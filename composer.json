{
    "name": "nuwira/gembok",
    "description": "Gembok is password token generation using number code.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Zamroni",
            "email": "zam@nuwira.co.id"
        }
    ],
    "require": {
        "laravel/framework": "5.4.*",
        "hashids/hashids": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "Nuwira\\Gembok\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nuwira\\Gembok\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nuwira\\Gembok\\GembokServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
