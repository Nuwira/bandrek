{
    "name": "nuwira/gembok",
    "description": "Gembok is password token generation using number code.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Zamroni",
            "email": "zam@nuwira.co.id"
        }
    ],
    "require": {
        "illuminate/auth": "5.4.*",
        "illuminate/support": "5.4.*",
        "illuminate/notifications": "5.4.*",
        "illuminate/database": "5.4.*",
        "hashids/hashids": "^2.0",
        "nesbot/carbon": "^1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "Nuwira\\Gembok\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nuwira\\Gembok\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
